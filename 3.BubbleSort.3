def Bubble_sort( lista ):
    vet=lista.copy()
    for i in range(len(vet)-1, 0, -1):
        prim_troca=0
        l=False
        for j in range(prim_troca, i, 1):
            if vet[j]>vet[j+1]:
                aux=vet[j]
                vet[j]=vet[j+1]
                vet[j+1]=aux
                if l is False and j==0:
                    prim_troca = j-1
                l=True
        print(vet)
        if l is False:
            return vet
    return vet
