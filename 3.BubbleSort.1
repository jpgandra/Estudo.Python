def Bubble_sort( lista ):
    vet=lista.copy()

    for i in range(len(vet)-1, 0, -1):
        for j in range(0, i, 1):
            if vet[j]>vet[j+1]:
                aux=vet[j]
                vet[j]=vet[j+1]
                vet[j+1]=aux
        print(vet)
    return vet
lista=[0,2,3,6,4,5,1,7,8,9,11,12, 14, 13, 15, 17,10, 16]
lista2=Bubble_sort(lista)

for i in range(len(lista)):
    print(lista[i], "\n")
print("--------------------------------------------------------")
for i in range(len(lista2)):
    print(lista2[i], "\n")
